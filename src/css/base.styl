.channel-list
  flex(0 0 180px)
  margin: 0 5px 0 0

  button.small
    float: right
    margin-right: -2px

.chat-box
  flex(1)
  display-flex()
  flex-direction(column)
  min-height: 1px

.chat-container
  flex(2)
  display-flex()
  flex-direction(column)

  .chat-card-zoom
