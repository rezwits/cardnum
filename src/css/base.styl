      .card-search
        position: relative

        .lookup
          width: 299px
          margin-right: 5px

        .typeahead
          position: absolute
          padding: 2px
          margin: 2px 0 0 0
          width: 308px
          background-color: rgba(255, 255, 255, 0.9)
          color: black
          border-radius: 4px
          border: 1px solid transparent-blue

          > div
            padding: 2px 5px
            border-radius: 4px
            border: 1px solid transparent
            cursor: pointer

            &:hover
              background-color: transparent-orange
