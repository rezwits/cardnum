{
  "name": "meccg",
  "version": "0.0.1",
  "license": "MIT",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/rezwits/cardnum.git"
  },
  "scripts": {
    "start": "coffee server.coffee",
    "postinstall": "bower install",
    "fetch": "cd data/ && coffee fetch.coffee",
    "promo": "cd data/ && mongo meccg --eval \"db.altarts.drop()\" && mongoimport --db=meccg --collection=altarts --file=promo.json --jsonArray && coffee add_art.coffee",
    "rotate": "cd data/ && coffee rotate.coffee",
    "link_cards": "cd data/ && coffee link_cards.coffee",
    "add_art": "cd data/ && coffee add_art.coffee"
  },
  "devDependencies": {
    "bower": "^1.8.2",
    "coffee-script": "^1.11.0"
  },
  "dependencies": {
    "async": "^2.0.1",
    "bcrypt": "^1.0.3",
    "body-parser": "^1.18.2",
    "connect-mongo": "^1.3.2",
    "cookie-parser": "^1.4.3",
    "cors": "^2.8.1",
    "diacritics": "^1.3.0",
    "express": "^4.14.0",
    "express-session": "^1.14.1",
    "jsonwebtoken": "^8.0.1",
    "memory-cache": "^0.2.0",
    "method-override": "^2.3.6",
    "mkdirp": "^0.5.1",
    "moment": "^2.15.1",
    "mongodb": "^2.2.10",
    "mongoskin": "^2.1.0",
    "node-trello": "^1.3.0",
    "node-uuid": "^1.4.7",
    "nodemailer": "^4.1.0",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "pug": "^2.0.0-rc.4",
    "request": "^2.82.0",
    "serve-favicon": "^2.4.4",
    "socket.io": "~1.3.7",
    "stylus": "^0.54.5",
    "zmq": "^2.15.3"
  }
}
